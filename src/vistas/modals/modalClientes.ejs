<div id="modalClientes" class="modal">
    <div class="modal-content">
        <h4>Clientes</h4>
        <form class="row">
            <div class="input-field col s12">
                <input type="text" name="mesa">
                <label for="mesa">Mesa</label>
            </div>
            <div class="input-field col s6">
                <input type="text" name="nombre">
                <label for="nombre">Nombre</label>
            </div>
            <div class="input-field col s6">
                <input type="text" name="apellido">
                <label for="apellido">Apellido</label>
            </div>
            <div class="input-field col s6">
                <select name="sexo">
                    <option value="" disabled selected>Sexo</option>
                    <option value="H">Hombre</option>
                    <option value="M">Mujer</option>
                </select>
                <label for="sexo">Sexo</label>
            </div>
            <div class="input-field col s6">
                <input type="text" name="telefono">
                <label for="telefono">Telefono</label>
            </div>
            <div class="input-field col s12">
                <input type="text" name="domicilio">
                <label for="domicilio">Domicilio</label>
            </div>
            <div class="input-field col s12">
                <input type="text" name="rfc">
                <label for="rfc">RFC</label>
            </div>
            <div class="col 12 right">
                <button type="submit" class="waves-effect waves-light btn-small green">Guardar</button>
                <button type="reset" class="waves-effect waves-light btn-small red">Cancelar</button>
            </div>
        </form>
    </div>
    <div class="modal-footer">
        <a href="#!" class="modal-close waves-effect waves-green btn-flat">Cerrar</a>
    </div>
</div>
